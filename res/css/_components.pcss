.whoistyping-loader {
}
@keyframes l1 {
    100% {
        box-shadow: 0 0 0 30px #0000;
    }
}

.mx_HomePage_default {
    .mx_HomePage_default_wrapper {
        margin: auto auto 0 auto;
    }

    img {
        height: 100px;
    }

    .mx_HomePage_default_buttons {
        display: flex;
        flex-direction: row;
        flex-wrap: wrap;
        padding: 30px;
        margin: 60px auto 0;
        width: fit-content;
    }
}

.mx_SpacePanel {
    .mx_UserMenu {
        padding: 0 2px 8px;
        border-bottom: 1px solid $separator;
        margin: 12px 14px 12px 18px;
        max-width: 226px;
    }
}

.mx_MessageComposer_wrapper {
    vertical-align: middle;
    margin: auto;
    position: relative;
    padding-left: 42px;
    padding-right: 16px;
}

.mx_LeftPanel_wrapper {
    display: flex;
    flex-direction: row;
    flex: 1;
    height: 100%; /* ensure space panel is still scrollable with an outer wrapper */

    .mx_LeftPanel_wrapper--user {
        background-color: $roomlist-bg-color;
        display: flex;
        overflow: hidden;
        position: relative;
        max-width: 350px;

        &[data-collapsed] {
            max-width: var(--collapsedWidth);
        }
    }
}

.mx_JumpToBottomButton {
    z-index: 40;
    position: absolute;
    /* 12 because height is 50 but button is only 38 = 12+(50-38) = 24 */
    bottom: 12px;
    right: 24px;
    width: 38px;
    /* give it a fixed height so the badge doesn't make */
    /* it taller and pop upwards when visible */
    height: 50px;
    text-align: center;
}

.mx_EventTile[data-layout="bubble"] {
    .mx_EventTile_line.mx_EventTile_sendByZebra {
        width: 95%;
        max-width: 95%;
        line-height: 24px;
    }
}

.mx_EventTile_content .markdown-body {
    font: 400 0.9375rem/1.5 Rubik !important;
    font: var(--cpd-font-body-md-regular) !important;
    letter-spacing: -0.0088em;
    letter-spacing: var(--cpd-font-letter-spacing-body-md);
    font-family: inherit !important;
    white-space: normal !important;
    line-height: 24px !important;
    color: inherit;
}

.mx_ResizeHandle {
    cursor: row-resize;
    flex: 0 0 auto;
    z-index: 100;

    &.mx_ResizeHandle--horizontal {
        margin: 0 -5px;
        padding: 0 5px;
        cursor: pointer;

        > div {
            width: 1px;
            height: 100%;
        }
    }

    &.mx_ResizeHandle--vertical {
        margin: -5px 0;
        padding: 5px 0;

        > div {
            height: 1px;
        }
    }
}

.mx_MatrixChat > .mx_ResizeHandle--to-left {
    &:hover {
        position: relative;

        &::before,
        &::after {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            width: 4px;
            border-radius: 4px;
            content: " ";
            background-color: $primary-content;
            opacity: 0.8;
        }

        &::before {
            margin-bottom: 12px;
            height: 14px; /* Height of the top segment */
            transform-origin: bottom center;
            transform: rotate(20deg);
        }

        &::after {
            margin-top: 12px;
            height: 14px; /* Height of the bottom segment */
            transform-origin: top center;
            transform: rotate(-20deg);
        }
    }
}

.mx_MatrixChat > .mx_ResizeHandle--to-right {
    &:hover {
        position: relative;

        &::before,
        &::after {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            width: 4px;
            border-radius: 4px;
            content: " ";
            background-color: $primary-content;
            opacity: 0.8;
        }

        &::before {
            margin-bottom: 12px;
            height: 14px; /* Height of the top segment */
            transform-origin: bottom center;
            transform: rotate(-20deg);
        }

        &::after {
            margin-top: 12px;
            height: 14px; /* Height of the bottom segment */
            transform-origin: top center;
            transform: rotate(20deg);
        }
    }
}

.mx_RoomSearch {
    flex: 1;
    min-width: 0;
    border-radius: 8px;
    background-color: $panel-actions;
    /* keep border thickness consistent to prevent movement */
    border: 1px solid transparent;
    height: 32px;
    padding: 4px;

    /* Create a flexbox for the icons (easier to manage) */
    display: flex;
    align-items: center;

    cursor: pointer;
}

.mx_RoomListHeader {
    .mx_RoomListHeader_plusButton {
        width: 32px;
        height: 32px;
        border-radius: 8px;
        position: relative;
        padding: 8px;
        margin: 8px;
        background-color: $panel-actions;
        box-sizing: border-box;
        flex-shrink: 0;
    }
}
.mx_SpacePanel {
    .mx_SpaceButton {
        &.mx_SpaceButton_home,
        &.mx_SpaceButton_favourites,
        &.mx_SpaceButton_people,
        &.mx_SpaceButton_orphans {
            .mx_SpaceButton_icon {
                background-color: rgba(0, 0, 0, 0);
            }
        }
    }
}

.mx_MessageComposer_e2eIcon.mx_E2EIcon {
    position: absolute;
    top: 50%;
    transform: translate(0%, -50%);
    margin-right: 0; /* Counteract the E2EIcon class */
    margin-left: 3px; /* Counteract the E2EIcon class */
    width: 12px;
    height: 12px;
}

.mx_SpacePanel {
    .mx_SpaceButton {
        .mx_SpaceButton_icon {
            width: var(--height-topLevel);
            min-width: var(--height-topLevel);
            height: var(--height-topLevel);
            border-radius: 8px;
            position: relative;

            &::before {
                position: absolute;
                content: "";
                width: var(--height-topLevel);
                height: var(--height-topLevel);
                top: 0;
                left: 0;
                mask-position: center;
                mask-repeat: no-repeat;
                mask-size: 24px;
            }
        }
    }
}
